#ifndef DUDA_VERSION_H
#define DUDA_VERSION_H

/* Helpers to convert/format version string */
#define STR_HELPER(s)      #s
#define STR(s)             STR_HELPER(s)

/* Fluent Bit Version */
#define DUDA_VERSION_MAJOR   @DUDA_VERSION_MAJOR@
#define DUDA_VERSION_MINOR   @DUDA_VERSION_MINOR@
#define DUDA_VERSION_PATCH   @DUDA_VERSION_PATCH@
#define DUDA_VERSION         (DUDA_VERSION_MAJOR * 10000 \
                             DUDA_VERSION_MINOR * 100   \
                             DUDA_VERSION_PATCH)
#define DUDA_VERSION_STR     "@DUDA_VERSION_STR@"

#endif
